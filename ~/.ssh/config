# 기본 설정
Host github.com
  HostName ssh.github.com
  Port 443
  User git
  IdentityFile ~/.ssh/min
  IdentitiesOnly yes

# hexaveil
Host github.hv
  HostName github.com
  User git
  IdentityFile ~/.ssh/hexaveil
  IdentitiesOnly yes

#min home server
Host monsil-server
  HostName monsilserver.iptime.org
  Port 2222
  User min
  IdentityFile ~/.ssh/monsil 